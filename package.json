{
  "name": "birds-api",
  "version": "1.0.0",
  "description": "A Public Restful API server built using Express.js and documented via Swagger, To retrieve and manipulate birds data",
  "main": "./dist/index.js",
  "author": "Amjed Omar <amjedomar201@gmail.com>",
  "repository": "https://github.com/amjedomar/birds-api",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "atco -w --exec \"node ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "build": "atco",
    "migration:create": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">= 12.16.0",
    "yarn": ">= 1.22.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mysql2": "^2.3.3",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "atco": "^1.0.13",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "tags": [
    "Express.js",
    "Swagger",
    "TypeORM",
    "TypeScript",
    "MySQL",
    "MariaDB"
  ]
}
